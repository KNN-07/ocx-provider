{
	// OCX Provider Registry
	// Proxied endpoints for Claude, Google, and GPT models
	//
	// Usage: ocx add norman-proxy/claude-proxy
	// Or:    ocx add norman-proxy/all-providers

	"$schema": "https://ocx.kdco.dev/schemas/registry.json",
	"name": "Norman's Proxy Providers",
	"namespace": "norman-proxy",
	"version": "0.0.1",
	"author": "Norman",

	"opencode": "1.0.0",
	"ocx": "1.0.16",

	"components": [
		// =============================================================================
		// INDIVIDUAL PROVIDERS
		// =============================================================================

		{
			"name": "claude-proxy",
			"type": "ocx:bundle",
			"description": "Proxied Claude models (Opus 4.5, Sonnet 4.5, with thinking variants)",
			"files": ["skill/proxy-setup/SKILL.md"],
			"opencode": {
				"provider": {
					"cliproxy": {
						"name": "Proxy API (Claude)",
						"npm": "@ai-sdk/anthropic",
						"options": {
							"apiKey": "${PROXY_API_KEY}",
							"baseURL": "${PROXY_BASE_URL}/v1",
							"timeout": 600000
						},
						"models": {
							"claude-opus-4-5-thinking": {
								"name": "Claude Opus 4.5 Thinking",
								"limit": {
									"context": 200000,
									"output": 64000
								},
								"reasoning": true,
								"options": {
									"thinking": {
										"type": "enabled",
										"budgetTokens": 32000
									}
								},
								"modalities": {
									"input": ["text", "image"],
									"output": ["text"]
								}
							},
							"claude-sonnet-4-5": {
								"name": "Claude Sonnet 4.5",
								"limit": {
									"context": 200000,
									"output": 64000
								},
								"modalities": {
									"input": ["text", "image"],
									"output": ["text"]
								}
							},
							"claude-sonnet-4-5-thinking": {
								"name": "Claude Sonnet 4.5 Thinking",
								"limit": {
									"context": 200000,
									"output": 64000
								},
								"reasoning": true,
								"options": {
									"thinking": {
										"type": "enabled",
										"budgetTokens": 32000
									}
								},
								"modalities": {
									"input": ["text", "image"],
									"output": ["text"]
								}
							}
						}
					}
				}
			}
		},

