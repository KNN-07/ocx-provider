{
	"buildCommand": "bunx ocx build . --out dist",
	"outputDirectory": "dist",
	"framework": null,
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET, OPTIONS"
				}
			]
		}
	]
}
